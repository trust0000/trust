[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/trust0000
[access "refs/for/refs/*"]
	read = group user/trust0000
	push = group user/trust0000
	label-Code-Review = -2..+2 group user/trust0000
	label-Verified = -1..+1 group user/trust0000
	rebase = group user/trust0000
	removeReviewer = group user/trust0000
	submit = group user/trust0000
[access "refs/heads/*"]
	read = group user/trust0000
	create = group user/trust0000
	push = +force group user/trust0000
	pushMerge = group user/trust0000
[access "refs/tags/*"]
	push = +force group user/trust0000
	pushTag = group user/trust0000
	pushSignedTag = group user/trust0000
